{
  "name": "serverless-graphql-apollo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql-apollo.git"
  },
  "scripts": {
    "test": "jest",
    "start-client-local": "cd ../app-client/ && env-cmd ../config/security.env.local yarn start",
    "start-client-prod": "cd ../app-client/ && env-cmd ../config/security.env.prod yarn start",
    "start-server": "env-cmd ../config/security.env.dev serverless offline start",
    "deploy-prod": "serverless --stage=production deploy",
    "deploy-dev": " env-cmd ../config/security.env.dev sls --stage=development --region=us-west-2 deploy"
  },
  "dependencies": {
    "apollo-server-lambda": "^1.2.0",
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.25.0",
    "graphql": "0.10.5",
    "graphql-playground-middleware": "^1.1.0",
    "knex": "^0.14.0",
    "lodash.get": "^4.4.2",
    "node-fetch": "^1.7.3",
    "oauth": "^0.9.15",
    "pg": "6",
    "pg-pool": "^2.0.3",
    "postgraphql": "^3.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-graphql": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "cors": "2.8.4",
    "env-cmd": "^5.0.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "serverless-offline": "^3.15.3",
    "serverless-webpack": "^2.2.0",
    "webpack": "^2.2.0",
    "webpack-node-externals": "^1.6.0"
  }
}
